module Assignment


type AMPM = AM | PM

let areHoursInvalid h = 
  if h < 1 || h > 12 then
    true
  else
    false


let areMinutesInvalid m =
  if m < 1 || m > 59 then
    true
  else
    false

let time h m ampm :(12 * 0 * AMPM) =
  (h, m, ampm)

let lessThan (time1: h * m * AMPM) (time2: h * m * AMPM) :bool =
  false
